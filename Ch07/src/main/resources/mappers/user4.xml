<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.ch07.dao.User4DAO">
	<select id="selectUser4List" resultType="kr.co.ch07.vo.User4VO">
		SELECT * FROM `user4`;
	</select>
	<select id="selectUser4One" resultType="kr.co.ch07.vo.User4VO">
		SELECT * FROM `user4` WHERE `seq` = #{seq};
	</select>
	<insert id="insertUser4">
		INSERT INTO `user4` SET `name` = #{name}, `gender` = #{gender}, `age` = #{age}, `addr` = #{addr};
	</insert>
	<update id="updateUser4">
		UPDATE `user4` SET `name` = #{name}, `gender` = #{gender}, `age` = #{age}, `addr` = #{addr} WHERE `seq` = #{seq};
	</update>
	<delete id="deleteUser4">
		DELETE FROM `user4` WHERE `seq` = #{seq};
	</delete>
</mapper>