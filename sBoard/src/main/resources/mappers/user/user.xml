<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.sboard.dao.UserDAO">
	<insert id="insertUser">
		insert into `board_user` set 
									`uid` = #{uid}, 
									`pass` = #{pass},
									`name` = #{name},
									`nick` = #{nick},
									`email` = #{email},
									`hp` = #{hp},
									`zip` = #{zip},
									`addr1` = #{addr1},
									`addr2` = #{addr2},
									`regip` = #{regip},
									`rdate` = NOW();
	</insert>
</mapper>